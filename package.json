{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "axios": "^1.8.4",
    "bootstrap": "^5.3.3",
    "chart.js": "^4.4.8",
    "cra-template": "1.3.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.4.0",
    "react-scripts": "5.0.1",
    "sweetalert2": "^11.17.2",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.js",
          "**/*.jsx"
        ],
        "rules": {
          "react/react-in-jsx-scope": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5001",
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1"
  }
}
